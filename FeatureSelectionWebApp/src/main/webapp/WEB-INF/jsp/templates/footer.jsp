<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>

<div class="row">
	<div id="cssValidation" class="col-sm-4 text-center">
		<a href="http://jigsaw.w3.org/css-validator/check/referer"> <img
			style="border: 0; width: 88px; height: 31px"
			src="http://jigsaw.w3.org/css-validator/images/vcss-blue"
			alt="Valid CSS!" />
		</a>
	</div>
	<div id="html5" class="col-sm-4 text-center">
		<a href="http://www.w3.org/html/logo/"> <img
			src="http://www.w3.org/html/logo/badge/html5-badge-h-css3-semantics.png"
			width="165" height="64"
			alt="HTML5 Powered with CSS3 / Styling, and Semantics"
			title="HTML5 Powered with CSS3 / Styling, and Semantics">
		</a>
	</div>
	<div id="hikari" class="col-sm-4 text-center">
		<a href="http://brettwooldridge.github.io/HikariCP/">
			<img src="<c:url value='/resources/images/hikari.png'/>"
				width="64" height="64"
				alt="Hikari Logo"/>
		</a>
	</div>
	<div id="tiles" class="col-sm-4 text-center">
		<a href="https://tiles.apache.org/">
			<img src="<c:url value='/resources/images/apachetiles.png'/>"
				width="165" height="64"
				alt="Apache Tiles Logo"/>
		</a>
	</div>
	<div id="spring" class="col-sm-4 text-center">
		<a href="https://spring.io/projects">
			<img src="<c:url value='/resources/images/spring-by-pivotal.png'/>"
				width="165" height="64"
				alt="Spring Logo"/>
		</a>
	</div>
	<div id="bootstrap" class="col-sm-4 text-center">
		<a href="http://www.getbootstrap.com/">
			<img src="<c:url value='/resources/images/twitter-bootstrap.jpg'/>"
				width="165" height="64"
				alt="Twitter Bootstrap Logo"/>
		</a>
	</div>
	
</div>